<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/simple_onto.owl"
         xmlns="http://example.org/simple_onto.owl#">

<owl:Ontology rdf:about="http://example.org/simple_onto.owl"/>

<owl:DatatypeProperty rdf:about="#hasColor">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Bird"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Animal">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Bird">
  <rdfs:subClassOf rdf:resource="#Animal"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#parrot">
  <rdf:type rdf:resource="#Bird"/>
  <hasColor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">green</hasColor>
</owl:NamedIndividual>


</rdf:RDF>
