@online{rampasekRecipeGeneralPowerful2023,
  title = {Recipe for a {{General}}, {{Powerful}}, {{Scalable Graph Transformer}}},
  author = {Ramp√°≈°ek, Ladislav and Galkin, Mikhail and Dwivedi, Vijay Prakash and Luu, Anh Tuan and Wolf, Guy and Beaini, Dominique},
