@online{gavranovicFundamentalComponentsDeep2024a,
  title = {Fundamental {{Components}} of {{Deep Learning}}: {{A}} Category-Theoretic Approach},
  shorttitle = {Fundamental {{Components}} of {{Deep Learning}}},
  author = {Gavranoviƒá, Bruno},
