@online{crawshawSLAWScaledLoss2021,
  title = {{{SLAW}}: {{Scaled Loss Approximate Weighting}} for {{Efficient Multi-Task Learning}}},
  shorttitle = {{{SLAW}}},
  author = {Crawshaw, Michael and Ko≈°eck√°, Jana},
  date = {2021-09-16},
  eprint = {2109.08218},
